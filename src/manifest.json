{
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_title": "__MSG_extensionName__",
    "theme_icons": [
      {
        "dark": "img/icon.svg#context",
        "light": "img/icon.svg#context-light",
        "size": 16
      },
      {
        "dark": "img/icon.svg#context",
        "light": "img/icon.svg#context-light",
        "size": 32
      }
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jid1-WiAigu4HIo0Tag@jetpack",
      "strict_min_version": "89.0"
    }
  },
  "commands": {
    "execEditor": {
      "description": "__MSG_commandExecEditor__",
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    },
    "openOptionsPage": {
      "description": "__MSG_commandOpenOptions__",
      "suggested_key": {
        "default": "Alt+Shift+U"
      }
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/content.js"
      ],
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/withExEditor",
  "icons": {
    "32": "img/icon.svg#current",
    "64": "img/icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "optional_permissions": [
    "notifications"
  ],
  "options_ui": {
    "browser_style": true,
    "open_in_tab": true,
    "page": "html/options.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "nativeMessaging",
    "storage",
    "tabs"
  ],
  "short_name": "__MSG_extensionName__",
  "version": "10.0.0"
}
