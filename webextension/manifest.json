{
  "applications": {
    "gecko": {
      "id": "jid1-WiAigu4HIo0Tag@jetpack",
      "strict_min_version": "52.0a1"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": "./img/icon.svg#gray",
    "default_title": "withExEditor"
  },
  "commands": {
  },
  "content_scripts": [
    {
      "all_frames": false,
      "matches": ["<all_urls>"],
      "js": ["./js/content.js"],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/withExEditor",
  "icons": {
    "48": "./img/icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "options_ui": {
    "open_in_tab": true,
    "page": "options/options.html"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "nativeMessaging",
    "notifications",
    "storage",
    "tabs",
    "webRequest"
  ],
  "version": "2.0.0"
}
